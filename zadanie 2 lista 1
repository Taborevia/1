#include <stdio.h>
#include <math.h>

int main() {
  float a,b,c,delta;
  scanf("%f",&a);
  scanf("%f",&b);
  scanf("%f",&c);
  delta= b*b - 4*a*c;
  if (delta==0)
  {
  printf("jedno rozwiazanie x: %2f",-b/(2*a));
  }
  else 
    {
    if (delta>0)
      {
      printf("pierwsze rozwiazanie x: %2f",(-b-sqrt(delta)/(2*a)));
      printf("drugie rozwiazanie x: %2f",(-b+sqrt(delta)/(2*a))); 
      }
    else 
      {
      printf("nie ma rozwiazan");
      }
    }
  return 0;
}
